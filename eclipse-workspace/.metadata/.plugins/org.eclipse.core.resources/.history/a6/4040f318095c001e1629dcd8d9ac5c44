package thread_ex;

public class ThreadEx1 {

	public static void main(String[] args) {
		
		
		for ( int i = 0 ; i < 5 ; i++ ) {
			System.out.println("띵");
			
//			try {
//				
//			} catch() {
//				
//			}
			
			Thread.sleep(1000);
		}
		
		System.out.println("==================");
		
		for ( int i = 0 ; i < 5 ; i++ ) {
			System.out.println("땅");
		}
		
	}

}
